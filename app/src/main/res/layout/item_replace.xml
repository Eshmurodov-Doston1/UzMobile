<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_80sdp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/code_card"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHeight_percent="0.8"
        app:layout_constraintWidth_percent="0.22"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="0dp"
        android:background="@drawable/item_back"
        android:elevation="@dimen/_3sdp"
        android:padding="@dimen/_3sdp"
        android:layout_height="0dp">
        <TextView
            android:id="@+id/code"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:textColor="#01B4FF"
            android:gravity="center"
            android:textSize="@dimen/_10sdp"
            android:textStyle="normal"
            android:textAllCaps="true"
            android:text="+998955040909"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.cardview.widget.CardView
        android:elevation="@dimen/_3sdp"
        android:foreground="?selectableItemBackground"
        android:clickable="true"
        android:id="@+id/click_op"
        app:cardCornerRadius="@dimen/_5sdp"
        android:focusable="true"
        app:layout_constraintTop_toTopOf="@id/code_card"
        app:layout_constraintBottom_toBottomOf="@id/code_card"
        app:layout_constraintLeft_toRightOf="@id/code_card"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/item_back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintWidth_percent="0.69"
        app:layout_constraintHorizontal_bias="0.9"
        android:layout_width="0dp"
        android:layout_height="0dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.2"
                app:layout_constraintWidth_percent="0.9"
                app:layout_constraintHorizontal_bias="0.3"
                android:id="@+id/info"
                android:text="cascascsdccsacscascsaccascsaccsacascscascsdacsacasc"
                android:gravity="start"
                android:textAllCaps="true"
                android:maxLines="2"
                android:textSize="@dimen/_11sdp"
                android:textColor="#01B4FF"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/price"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="cscsdcdscdscdscdscdscsdcsdcvdscadscadscacascsaccsacascsacascascascascasccsacsa"
                android:textSize="@dimen/_9sdp"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="@id/info"
                app:layout_constraintRight_toRightOf="@id/info"
                app:layout_constraintTop_toBottomOf="@id/info" />
            <TextView
                android:id="@+id/count"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:text="cscsdcdscdscdscdscdscsdcsdcvdscadscadscacascsaccsacascsacascascascascasccsacsa"
                android:textSize="@dimen/_9sdp"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintLeft_toLeftOf="@id/info"
                app:layout_constraintRight_toRightOf="@id/info"
                app:layout_constraintTop_toBottomOf="@id/price" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>