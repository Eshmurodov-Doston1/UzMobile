<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/dialog"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="@dimen/_140sdp">
        <TextView
            android:id="@+id/phone_number"
            android:maxLines="1"
            android:maxLength="14"
            android:text="+998994206278"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.1"
            app:layout_constraintVertical_bias="0.1"
            android:textColor="@color/black"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/info"
            android:textColor="@color/black"
            android:text="sancbknsdzcl,dsncsd"
            android:textSize="@dimen/_12sdp"
            android:textStyle="normal"
            app:layout_constraintTop_toBottomOf="@id/phone_number"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="@id/phone_number"
            app:layout_constraintWidth_percent="0.88"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintVertical_bias="0.2"
            android:layout_width="0dp"
            android:layout_height="0dp"/>
        <TextView
            android:id="@+id/phon_call"
            android:textStyle="normal"
            android:textSize="@dimen/_10sdp"
            android:padding="@dimen/_5sdp"
            android:textAllCaps="true"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/info"
            android:textColor="#01B4FF"
            android:text="Qo`ng`iroq qilish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/clouse"
            android:textStyle="normal"
            android:textSize="@dimen/_10sdp"
            android:padding="@dimen/_5sdp"
            android:textAllCaps="true"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintRight_toLeftOf="@id/phon_call"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/info"
            android:textColor="#01B4FF"
            android:text="orqaga"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>