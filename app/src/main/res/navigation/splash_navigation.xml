<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_navigation"
    app:startDestination="@id/splashOpenFragment">
    <fragment
        android:id="@+id/splashOpenFragment"
        android:name="com.example.uzmobile.ui.splashFragments.SplashOpenFragment"
        android:label="fragment_splash_open"
        tools:layout="@layout/fragment_splash_open" >
        <action
            android:id="@+id/action_splashOpenFragment_to_languageFragment"
            app:destination="@id/languageFragment" />
    </fragment>
    <fragment
        android:id="@+id/languageFragment"
        android:name="com.example.uzmobile.ui.splashFragments.LanguageFragment"
        android:label="fragment_language"
        tools:layout="@layout/fragment_language" />
</navigation>